{
    "routes": [
      { "src": "^/$", "dest": "/index.html" },
      { "src": "^/sobre$", "dest": "/index.html" },
      { "src": "^/noticias$", "dest": "/index.html" },
      { "src": "^/noticias/.*$", "dest": "/index.html" },
      { "src": "^/calendario$", "dest": "/index.html" }
    ],
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "cleanUrls": true,
    "trailingSlash": false
}